# Example production env file for syngonium
# Copy this file to /etc/default/syngonium and update values.
# Keep this file out of version control if it contains secrets.

ENVIRONMENT=production
SLACK_BOT_TOKEN=""
SLACK_APP_TOKEN=""
AI_API_KEY=""
AI_API_BASE="https://api.example.com"
AI_MODEL="qwen/qwen3-32b"
BOT_NAME="Assistant"
FAQ_LINK=""
AI_VALIDATION_MODEL="x-ai/grok-4.1-fast"
AI_MAX_RETRIES=2
LOCAL_DOCS_PATH="faq.md"
HEALTH_PORT=8000
AI_MAX_WORKERS=5
AI_MAX_RPS=20
AI_RPS_CAPACITY=40
AI_CIRCUIT_FAILS=6
AI_CIRCUIT_RECOVERY=60
LISTEN_CHANNEL_ID=""
CHECK_CHANNELS=""
INVITE_CHANNELS=""
INVITE_SYNC_INTERVAL_S=600
AI_DEBUG=false
SLACK_MANAGERS_GROUP_ID=S09TGU4FWNS

# Optional run.sh flags
#SKIP_AUTO_UPDATE=true
#SKIP_REQUIREMENTS=true
#SKIP_VENV=true
